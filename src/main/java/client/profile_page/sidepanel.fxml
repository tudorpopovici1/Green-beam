<VBox prefHeight="700" prefWidth="200" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.profile_page.SidePanelController">
    <children>
      <Pane prefHeight="127.0" prefWidth="200">
         <children>
              <ImageView fitHeight="127.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                      <Image url="@../mainpage/images/back.jpg" />
                  </image>
              </ImageView>
            <JFXTextField fx:id="searchFriends" focusColor="#f3a978" layoutX="4.0" layoutY="87.0" maxWidth="127.0" minWidth="127.0" prefHeight="31.0" prefWidth="127.0" promptText="Search for friends" />
            <JFXButton fx:id="addFriendsButton" layoutX="137.0" layoutY="90.0" styleClass="buttonsfriends" stylesheets="@profile.css" text="ADD">
               <font>
                  <Font name="Gotham-Bold" size="14.0" />
               </font>
            </JFXButton>
         </children>
      </Pane>
      <JFXListView fx:id="friendslistView" prefHeight="573.0" prefWidth="200.0" />
    </children>
</VBox>
