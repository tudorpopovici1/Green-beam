            <Image url="@../images/logo2.png" />
        </image>
    </ImageView>
    <AnchorPane layoutY="-7.0" prefHeight="713.0" prefWidth="200.0">
      <children>
          <Button fx:id="mainButton" layoutY="7.0" mnemonicParsing="false" prefHeight="109.0" prefWidth="200.0" text="Main">
